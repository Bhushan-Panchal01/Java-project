pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                sh """
                    docker build -t pythonimg:1.0 .
                """
            }
        }
        stage('Run'){
            steps{
                sh """
                    docker run -p 8090:5000 -d docker-flask pythonimg:1.0
                """
            }
        }
    }
}